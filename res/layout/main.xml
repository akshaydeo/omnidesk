<?xml version="1.0" encoding="utf-8"?>




<FrameLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<omniDesk.net.rdp.ImageViewer android:id="@+id/android_canvas"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:layout_gravity="bottom|center" />


	<Button android:id="@+id/Button01" android:width="10sp"
		android:gravity="top|center" android:layout_gravity="top|center"
		android:layout_height="40sp" android:layout_width="40sp" android:background="@drawable/fullscreen_unpressed" android:visibility="invisible" android:focusableInTouchMode="true" android:focusable="false"></Button>
</FrameLayout>
